    "randexp": "^0.5.3",
    "tsx": "^4.16.2"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.10",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "jest": "^29.5.0",
    "tsup": "^8.1.2",
    "typescript": "^5.5.3"
  }
