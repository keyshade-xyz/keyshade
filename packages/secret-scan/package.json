{
  "name": "@keyshade/secret-scan",
  "version": "1.0.0",
  "description": "Do static analysis of a string to find secrets",
  "main": "dist/index.js",
  "scripts": {
    "genKey": "tsx src/generateKey.ts",
    "build": "tsup",
    "test": "mocha",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
