{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
	  "build": {

		"dependsOn": ["^build"],

		"outputs": [".next/**", "!.next/cache/**"]
	  },
	  "deploy": {

		"dependsOn": ["build", "test", "lint"]
	  },
	  "test": {

		"dependsOn": ["build"],

		"inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
	  },

	  "lint": {},
	  "dev": {
		"cache": false,
		"persistent": true
	  }
	}
  }